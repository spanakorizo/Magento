<?xml version="1.0"?>
<config>
    <modules>
        <Compandsave_Variable>           
            <version>0.0.2</version>
        </Compandsave_Variable>
    </modules>
    <global>
        <models>
            <compandsave_variable>
				<class>Compandsave_Variable_Model</class>
				<resourceModel>compandsave_variable_resource</resourceModel>
            </compandsave_variable>
            <compandsave_variable_resource>
				<class>Compandsave_Variable_Model_Resource</class>
                <entities>
                    <coupon>
                        <table>compandsave_variable_coupon</table>
                    </coupon>
                </entities>
            </compandsave_variable_resource>
        </models>
		<blocks>
            <compandsave_variable_adminhtml>
               <class>Compandsave_Variable_Block_Adminhtml</class>
            </compandsave_variable_adminhtml>
        </blocks>
		<helpers>
            <compandsave_variable>
                <class>Compandsave_Variable_Helper</class>
            </compandsave_variable>
        </helpers>
        <resources>
            <compandsave_variable_setup>
                <setup>
                    <module>Compandsave_Variable</module>
                    <class>Compandsave_Variable_Model_Resource_Setup</class>
                </setup>
            </compandsave_variable_setup>
        </resources>
    </global>
	<admin>
        <routers>
            <compandsave_variable_admin>
                <use>admin</use>
                <args>
                   <module>Compandsave_Variable_Adminhtml</module>
                    <frontName>coupon-admin</frontName>
                </args>
            </compandsave_variable_admin>

        </routers>
    </admin>
</config>